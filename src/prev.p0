:fun(convertEscapableChar){
	y = x == 110 # n
	:if(y){
		y = 10 # newline
	} else {
		y = x == 116 # t
		:if(y){
			y = 9 # tab
		} else {
			y = x == 39 # '
			:if(y){
				y = 39
			} else {
				y = x == 34 # "
				:if(y){
					y = 34
				} else {
					y = x == 92 # \
					:if(y){
						y = 92
					} else {
						:throw("Invalid escape sequence. Line ")
						x = l
						:call(errint)
						:throw("\n")
						x = 1
						:exit(x)
					}
				}
			}
		}
	}
}
